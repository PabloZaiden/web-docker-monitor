<html>
    <body style="font-family: monospace">
        <p>
            Containers

            <ul>
                <%for (let container of model) { %>
                    <li>
                        <%=container.Image %> 
                        (<%=container.Status%>)
                        <a href="/docker/logs?id=<%=container.Id%>">View logs</a>
                        <%if (container.State !== "running") { %>
                            <a href="/docker/start?id=<%=container.Id%>">Start</a>
                        <%}%>
                    </li>
                <%}%>
            </ul>
        </p>
    </body>
</html>